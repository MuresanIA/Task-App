<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.MIA.controller.RegisterController"
            fx:id="registerLayout" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <children>
        <Label layoutX="30.0" layoutY="28.0" text="Username"/>
        <Label layoutX="30.0" layoutY="64.0" text="Password"/>
        <Label layoutX="30.0" layoutY="100.0" prefHeight="21.0" prefWidth="150.0" text="Re-enter password"/>
        <TextField fx:id="txtFieldUsernameRegister" layoutX="180.0" layoutY="23.0"/>
        <Button fx:id="btnRegister" layoutX="293.0" layoutY="139.0" mnemonicParsing="false"
                onAction="#registerUser" text="Register"/>
        <Button fx:id="btnPreviousScene" layoutX="200.0" layoutY="139.0" mnemonicParsing="false" onAction="#goBack"
                text="Back"/>
        <PasswordField fx:id="pwdFieldRegister" layoutX="180.0" layoutY="59.0"/>
        <PasswordField fx:id="pwdFieldConfirmRegister" layoutX="180.0" layoutY="95.0"/>
        <Label fx:id="lblInformationRegister" layoutX="14.0" layoutY="197.0" prefHeight="21.0"
               prefWidth="624.0"/>
    </children>
</AnchorPane>