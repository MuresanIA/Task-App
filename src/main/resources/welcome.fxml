<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.MIA.controller.WelcomeController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox>
        <Button fx:id="btnLogin" mnemonicParsing="false"
                onAction="#navigateToLoginScene" text="Login"/>

        <Button fx:id="btnRegister" mnemonicParsing="false"
                onAction="#navigateToRegisterScene" text="Register"/>
    </VBox>
  
</AnchorPane>
